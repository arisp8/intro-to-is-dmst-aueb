if(closeCnt<4){cancelCookieDuration=8*(closeCnt++);setCookie('closeCnt',closeCnt,30);}else{cancelCookieDuration=30;}}catch(e){cancelCookieDuration=1;}
setCookie('downloadAppAction','cancel',cancelCookieDuration);setTimeout(function(){hidePromoOverlay();},100);}
function iOSversion(){if(/iP(hone|od|ad)/.test(navigator.platform)){var v=(navigator.appVersion).match(/OS (\d+)_(\d+)_?(\d+)?/);return parseFloat(parseInt(v[1],10)+"."+parseInt(v[2],10)+"."+parseInt(v[3]||0,10));}}

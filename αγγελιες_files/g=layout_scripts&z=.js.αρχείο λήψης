function downloadApp(device){setCookie('downloadApp','visited',30);setCookie('downloadAppAction','download',30);if(device=='iphone'){gotoOutboundLink('https://itunes.apple.com/gr/app/spitogatos.gr-700.000+-angelies/id1057971912?mt=8&l=el');}else if(device=='android'){gotoOutboundLink('https://play.google.com/store/apps/details?id=gr.spitogatos.android');}else if(device=='blackberry'){gotoOutboundLink('http://appworld.blackberry.com/webstore/content/42152889/');}else if(device=='windowsPhone'){gotoOutboundLink('http://www.windowsphone.com/it-it/store/app/immobiliare-it/d97dfd3e-a042-47e8-8f76-3b2a41c1b564');}
setTimeout(function(){hidePromoOverlay();},500);}
